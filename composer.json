{
    "name": "mission10/craftcms-block-styles",
    "description": "Style dropdown for Neo blocks",
    "type": "craft-plugin",
    "license": "mit",
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.5.0|^5.0.0"
    },
    "autoload": {
        "psr-4": {
            "mission10\\blockstyles\\": "src/"
        }
    },
    "extra": {
        "handle": "block-styles",
        "name": "Block Styles",
        "documentationUrl": "",
        "class": "mission10\\blockstyles\\BlockStyles"
    },
    "authors": [
        {
            "name": "Mission10",
            "homepage": "https://mission10.co.uk"
        }
    ],
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

